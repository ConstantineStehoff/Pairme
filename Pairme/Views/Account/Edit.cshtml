@using Microsoft.AspNet.Identity
@model Pairme.Models.ApplicationUser

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_LoggedLayout.cshtml";
}

<h2>Edit Profile</h2>
<div class="row">
    <div class="col-md-4"><img src="http://placehold.it/350x150"></div>
    <div class="col-md-8"> 
        <h3>@User.Identity.GetUserName()</h3>
        <div>
            <span>@Model.Age year old, @Model.GenderID</span> 
        </div> 
    </div>
</div>

